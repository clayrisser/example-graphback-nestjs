# app
DEBUG=1
PORT=5000

# database
DATABASE_ENGINE=postgres
MIGRATE=1

# seed
SEED=1

# security
BCRYPT_SALT_OR_ROUNDS=10
CORS=1
CORS_ORIGIN="*"
SECRET=P@ssw0rd

# postgres
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DATABASE=postgres
POSTGRES_HOST=localhost
POSTGRES_PORT=5432

# redis
REDIS_DATABASE=0
REDIS_HOST=localhost
REDIS_PASSWORD=
REDIS_PORT=6379

# keycloak
KEYCLOAK_ADMIN_PASSWORD=P@ssw0rd
KEYCLOAK_ADMIN_USERNAME=admin
KEYCLOAK_CLIENT_ID=app
KEYCLOAK_CLIENT_SECRET=P@ssw0rd
KEYCLOAK_BASE_URL=http://localhost:8080
KEYCLOAK_REALM=main

# sqlite
SQLITE_URL=file:./sqlite.db

# swagger
SWAGGER=0
SWAGGER_BASE_PATH=/docs

# sofa
SOFA_BASE_PATH=/api

# graphql
GRAPHQL_PLAYGROUND=0
ENABLE_CACHING=1
DEFAULT_MAX_AGE=60

# telemetry
LOG_AXIOS_DATA=1
LOG_AXIOS_HEADERS=1
LOG_FILE_NAME=docker/data/logs/app.log
LOG_LEVELS=error,warn,log
LOG_PRETTY=0
OTEL_EXPORTER_JAEGER_ENDPOINT=http://localhost:14268/api/traces
OTEL_SERVICE_NAME=app
